{
  "name": "npm-version-mgmt",
  "version": "6.0.0-dev.2",
  "description": "Test project to learn npm version mgmt",
  "main": "app/index.js",
  "scripts": {
    "start": "node ./app/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ramkrao/npm-version-mgmt.git"
  },
  "author": "Ramkrao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ramkrao/npm-version-mgmt/issues"
  },
  "homepage": "https://github.com/ramkrao/npm-version-mgmt#readme",
  "devDependencies": {
    "@semantic-release/changelog": "github:semantic-release/changelog"
  },
  "dependencies": {
    "semantic-release": "^15.13.19",
    "semver": "^5.6.0"
  },
  "installConfig": {
    "pnp": true
  },
  "publishConfig": {
    "tag": "next"
  },
  "prepare": [
    "[@semantic](http://twitter.com/semantic)-release/changelog",
    "[@semantic](http://twitter.com/semantic)-release/npm",
    {
     "path": "[@semantic](http://twitter.com/semantic)-release/git",
      "assets": [
       "package.json",
       "package-lock.json",
       "CHANGELOG.md"
     ],
     "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
   }
 ]
}
